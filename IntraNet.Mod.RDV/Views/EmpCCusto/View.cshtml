@model IEnumerable<IntraNet.Domain.Entities.EmpCCusto>

@{
    ViewBag.Title = "View";
}
<label>Registros</label>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.empresa.RazaoSocial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CCusto)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CCustoDesc)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Ativo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.empresa.RazaoSocial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCusto)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CCustoDesc)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Ativo)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmpCCustoId }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmpCCustoId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmpCCustoId })
        </td>
    </tr>
}

</table>
